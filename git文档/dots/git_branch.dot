digraph G {
    rankdir = LR;
    node[shape=box, height=.1, width=.1];

    subgraph cluster_commit {
        label = "提交历史";
        node[style=filled, shape=box, height=.1, width=.1];

        a -> b -> c -> d -> e -> f -> g -> x -> y;
        a -> h -> c -> k -> i -> x;
        k -> j -> i;
    }

    subgraph cluster_branch {
        label = "分支";
        node[shape=box]

        br1, br2, br3;
    }

    HEAD [style=filled];
    HEAD -> br1 [label="正常"];
    HEAD -> b [style=dotted, label="分离头"];

    br1 -> a;
    br2 -> h;
    br3 -> h;

    tag1,tag2,tag3[shape=ellipse];

    tag1 -> c;
    tag2 -> f;
    tag3 -> i;
}

